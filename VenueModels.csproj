<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>disable</Nullable>
    <PackageId>FFXIVVenues.$(AssemblyName)</PackageId>
    <Authors>Kana Ki @ Gilgamesh</Authors>
    <Product>FFXIV Venues</Product>
    <PackageIcon>logo.png</PackageIcon>
    <RepositoryUrl>https://github.com/FFXIVVenues/ffxiv-venues-models</RepositoryUrl>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <AssemblyVersion>$(Version)</AssemblyVersion>
    <FileVersion>$(Version)</FileVersion>
    <Version>4.1.0</Version>
    <PackageTags>ffxiv; venues</PackageTags>
    <Description>Data models for FFXIV Venues (includes 2021 and 2022 models). </Description>
    <PackageReleaseNotes>Update Opening to use Location model for location property rather than string</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
	  <None Include="logo.png" Pack="true" PackagePath="" />
	  <None Include="LICENSE" Pack="true" PackagePath="" />
  </ItemGroup>

</Project>
